<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>1.0</Version>
    <Authors>Adam Fisher</Authors>
    <Company />
    <Description>A C# wrapper to decay values based on conditions.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/adamfisher/Decay</PackageProjectUrl>
    <PackageIcon>nuclear_icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/adamfisher/Decay</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>decay, lazy, expire, expiration</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\nuclear_icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>